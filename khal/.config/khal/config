[calendars]

[[personal_local]]
path = ~/.calendars/personalics/
type = discover
color = light green

[[uni_local]]
path = ~/.calendars/uniics/
type = discover
color = light red

[[alise_local]]
path = ~/.calendars/personal_shared_by_alise/
type = discover
color = light blue

[[hwp_local]]
path = ~/.calendars_uni/hwp/
type = discover
color = light cyan

[locale]
timeformat = %H:%M
dateformat = %d/%m/%Y
longdateformat = %d/%m/%Y
datetimeformat = %d/%m/%Y %H:%M
longdatetimeformat = %d/%m/%Y %H:%M
